<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.MainWindowController">
  <children>
    <MenuBar fx:id="menuBar" VBox.vgrow="NEVER">
      <menus>
        <Menu fx:id="fileMenu" mnemonicParsing="false" text="File">
          <items>
            <MenuItem fx:id="newProjectMI" mnemonicParsing="false" onAction="#newProjectAction" text="New" />
            <MenuItem fx:id="openProjectMI" mnemonicParsing="false" onAction="#openProjectAction" text="Openâ€¦" />
            <MenuItem fx:id="closeProjectMI" mnemonicParsing="false" onAction="#closeProjectAction" text="Close" />
            <MenuItem fx:id="saveProjectMI" mnemonicParsing="false" onAction="#saveProjectAction" text="Save" />
          </items>
        </Menu>
        <Menu fx:id="editMenu" mnemonicParsing="false" text="Edit">
          <items>
            <MenuItem fx:id="undoMI" mnemonicParsing="false" onAction="#undoAction" text="Undo" />
            <MenuItem fx:id="redoMI" mnemonicParsing="false" onAction="#redoAction" text="Redo" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem fx:id="cutMI" mnemonicParsing="false" onAction="#cutAction" text="Cut" />
            <MenuItem fx:id="copyMI" mnemonicParsing="false" onAction="#copyAction" text="Copy" />
            <MenuItem fx:id="pasteMI" mnemonicParsing="false" onAction="#pasteAction" text="Paste" />
            <MenuItem fx:id="deleteMI" mnemonicParsing="false" onAction="#deleteAction" text="Delete" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem fx:id="selectAllMI" mnemonicParsing="false" onAction="#selectAllAction" text="Select All" />
          </items>
        </Menu>
        <Menu fx:id="projectMenu" mnemonicParsing="false" text="Project">
          <items>
            <MenuItem fx:id="addTerminalMI" mnemonicParsing="false" onAction="#addTerminalAction" text="Add Terminal" />
                  <MenuItem fx:id="addEdgeMI" mnemonicParsing="false" onAction="#addEdgeAction" text="Add Edge" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <SplitPane dividerPositions="0.5" focusTraversable="true" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <items>
            <ScrollPane fx:id="projectViewPane" prefHeight="200.0" prefWidth="200.0" />
        <AnchorPane fx:id="projectPropertiesPane" maxWidth="220.0" minWidth="220.0" prefWidth="220.0">
               <children>
                  <Label layoutX="78.0" layoutY="17.0" text="Properties" AnchorPane.leftAnchor="76.0" AnchorPane.rightAnchor="76.0" AnchorPane.topAnchor="10.0" />
                  <ScrollPane fx:id="propertiesContainerPane" maxWidth="200.0" minWidth="200.0" prefHeight="504.0" prefWidth="200.0" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="30.0">
                     <content>
                        <AnchorPane fx:id="terminalPropertiesPane" maxWidth="208.0" minWidth="208.0" prefHeight="509.0" prefWidth="208.0">
                           <children>
                              <Label layoutX="14.0" layoutY="14.0" text="Terminal" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0" />
                              <Label layoutX="16.0" layoutY="33.0" text="Location:" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="30.0" />
                              <Label layoutX="10.0" layoutY="54.0" text="x:" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="55.0" />
                              <Label layoutX="37.0" layoutY="152.0" text="y:" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="85.0" />
                              <TextField fx:id="terminalXValue" layoutX="34.0" layoutY="60.0" prefWidth="170.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="50.0" />
                              <TextField fx:id="terminalYValue" layoutX="16.0" layoutY="172.0" prefHeight="26.0" prefWidth="170.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="80.0" />
                           </children>
                        </AnchorPane>
                     </content>
                  </ScrollPane>
               </children>
        </AnchorPane>
      </items>
    </SplitPane>
    <HBox id="HBox" alignment="CENTER_LEFT" spacing="5.0" VBox.vgrow="NEVER">
      <children>
        <Label fx:id="leftStatus" maxHeight="1.7976931348623157E308" maxWidth="-1.0" text="Left status" HBox.hgrow="ALWAYS">
          <font>
            <Font size="11.0" fx:id="x3" />
          </font>
          <textFill>
            <Color blue="0.625" green="0.625" red="0.625" fx:id="x4" />
          </textFill>
        </Label>
        <Pane prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
        <Label fx:id="rightStatus" font="$x3" maxWidth="-1.0" text="Right status" textFill="$x4" HBox.hgrow="NEVER" />
      </children>
      <padding>
        <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
      </padding>
    </HBox>
  </children>
</VBox>
